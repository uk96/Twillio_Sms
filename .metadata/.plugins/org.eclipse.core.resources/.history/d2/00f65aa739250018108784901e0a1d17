package com.Twilio;
import com.twilio.Twilio;
import com.twilio.rest.api.v2010.account.Message;
import com.twilio.type.PhoneNumber;

public class SendSms {
	
	public static final String ACCOUNT_SID = "ACa20d10960fbd2280329a873fd73bcedd";
	public static final String AUTH_TOKEN = "c13ceab87c94796bf19e116f06ec4b3c";
	public void sms(String number)
	{
		Twilio.init(ACCOUNT_SID, AUTH_TOKEN);
		Message message = Message.creator(new PhoneNumber("+919892326829"),new PhoneNumber("+18132854847"), "This is the ship that made the Kessel Run in fourteen parsecs?").create();
		System.out.println(message.getSid());
	}
}
